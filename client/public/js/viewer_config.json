  "weather_data": {
    "sources": {
      "noaa": {
        "base_url": "https://api.weather.gov",
        "endpoints": {
          "alerts": "/alerts/active",
          "stations": "/stations",
          "observations": "/stations/{station}/observations",
          "forecast": "/points/{lat},{lon}/forecast"
        },
        "refresh_interval": 300,
        "timeout": 10000
      },
      "nhc": {
        "base_url": "https://www.nhc.noaa.gov",
        "endpoints": {
          "active_storms": "/CurrentStorms.json",
          "gis_data": "/gis/"
        },
        "refresh_interval": 1800,
        "timeout": 15000
      }
    },
    "data_types": [
      "hurricane_tracks",
      "wind_probabilities",
      "storm_surge",
      "rainfall_forecasts",
      "weather_alerts",
      "satellite_imagery"
    ]
  },

  "visualization": {
    "quantum_arcs": {
      "enabled": true,
      "confidence_threshold": 0.5,
      "max_radius": 300000,
      "animation_speed": 1.0,
      "glow_intensity": 0.3
    },
    "hurricane_tracks": {
      "enabled": true,
      "track_width": 3,
      "point_size": 8,
      "uncertainty_bands": true,
      "historical_tracks": false
    },
    "weather_overlays": {
      "radar": {
        "enabled": true,
        "transparency": 0.7,
        "refresh_rate": 300
      },
      "satellite": {
        "enabled": true,
        "transparency": 0.6,
        "refresh_rate": 900
      },
      "pressure_systems": {
        "enabled": true,
        "contour_lines": true,
        "color_gradient": true
      }
    },
    "agent_network": {
      "orbital_visualization": true,
      "connection_lines": true,
      "activity_indicators": true,
      "status_panels": true
    }
  },

  "cyberpunk_theme": {
    "primary_color": "#00ffff",
    "secondary_color": "#ff6b6b",
    "accent_color": "#00ff88",
    "background_color": "#0a0a0f",
    "text_color": "#e0e0e0",
    "glow_effects": true,
    "neon_borders": true,
    "matrix_backgrounds": true,
    "font_family": "Inter, Courier New, monospace"
  },

  "performance": {
    "optimization": {
      "level_of_detail": true,
      "frustum_culling": true,
      "occlusion_culling": false,
      "batching": true
    },
    "rendering": {
      "multi_sample_anti_aliasing": 4,
      "high_dynamic_range": true,
      "bloom_effects": true,
      "depth_of_field": false
    },
    "memory": {
      "texture_streaming": true,
      "garbage_collection": true,
      "resource_pooling": true
    }
  },

  "arcsec_security": {
    "enabled": true,
    "data_integrity": {
      "hash_algorithm": "SHA-256",
      "verification_interval": 60,
      "integrity_threshold": 0.95
    },
    "authorship_tracking": {
      "digital_signatures": true,
      "provenance_chain": true,
      "timestamp_authority": "RFC3161"
    },
    "audit_logging": {
      "enabled": true,
      "retention_period": 2592000,
      "log_level": "info"
    }
  },

  "development": {
    "debug_mode": false,
    "performance_monitoring": true,
    "error_reporting": true,
    "feature_flags": {
      "experimental_features": false,
