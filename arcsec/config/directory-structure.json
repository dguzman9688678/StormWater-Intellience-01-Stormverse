    },
    "replit_local/": {
      "priority": 4,
      "description": "Quaternary - Replit-specific configurations",
      "note": "Environment-specific files, lowest priority"
    }
  },
  "migration_plan": {
    "phase_1": "Move core ARCSEC utilities to arcsec/utilities/",
    "phase_2": "Relocate security components to arcsec/security/",
    "phase_3": "Transfer agent services to arcsec/agents/",
    "phase_4": "Organize API services in arcsec/services/",
    "phase_5": "Update all import paths and references"
  },
  "protection_status": {
