      "description": "Primary ARCSEC ecosystem directory",
      "subdirectories": {
        "core/": {
          "purpose": "Core ARCSEC system components",
          "contains": ["controllers", "handlers", "processors"],
          "priority": "CRITICAL"
        },
        "security/": {
          "purpose": "Security protocols and monitoring",
          "contains": ["protocols", "monitoring", "compliance"],
          "priority": "MAXIMUM"
        },
        "agents/": {
          "purpose": "AI agent network and coordination", 
          "contains": ["network", "coordination", "models"],
          "priority": "HIGH"
        },
        "services/": {
          "purpose": "API services and system interfaces",
          "contains": ["api", "websocket", "database"],
          "priority": "HIGH"
        },
        "utilities/": {
          "purpose": "ARCSEC utility tools and scripts",
          "contains": ["tools", "scripts", "generators"],
          "priority": "MEDIUM"
        },
        "config/": {
          "purpose": "Configuration and manifest files",
          "contains": ["system", "security", "network"],
          "priority": "HIGH"
        },
        "data/": {
          "purpose": "Protected data storage",
          "contains": ["processed", "raw", "backups"],
          "priority": "HIGH"
        },
        "logs/": {
          "purpose": "System and security logging",
          "contains": ["system", "security", "audit"],
          "priority": "MEDIUM"
        },
        "backup/": {
          "purpose": "System backup and recovery",
          "contains": ["files", "configs", "databases"],
          "priority": "CRITICAL"
        }
      }
    },
    "server/": {
      "priority": 2,
      "description": "Secondary - Express.js server components",
      "note": "Integrates with ARCSEC primary structure"
    },
    "client/": {
      "priority": 3,
      "description": "Tertiary - React frontend components", 
